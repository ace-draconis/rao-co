<section class="swag text-center" style="background-image:url('{{ page.media[page.header.img].url }}')">
  <div class="container">
    <div class="row">
      <div class="col-md-8 col-md-offset-2">
        <h1>{{ page.title }}</h1>
        <a href="#{{ page.header.id }}" class="down-arrow-btn"><i class="fa fa-chevron-down"></i></a>
      </div>
    </div>
  </div>
</section>
<section class="intro  section-padding" id="reported_cases">
  <div class="container">
    <div class="row">
      <div class="col-md-12 text-center">
        <h1 class="arrow">{{ page.header.content_title }}</h1>
        <br> <br>
      </div>
    </div>
    <div class="row">
      <div class="col-md-4">
        <br>
        <img src="{{ page.media[page.header.content_img].url }}" class="img-responsive default">
        <br>
        <img src="{{ page.media[page.header.content_img_2].url }}" class="img-responsive default">
        <br>
        <img src="{{ page.media[page.header.content_img_3].url }}" class="img-responsive default">
      </div>
      <div class="col-md-8 cases">{{page.content}}</div>
    </div>
  </div>
</section>